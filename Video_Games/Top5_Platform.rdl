  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>video_games1</DataSourceName>
        <CommandText>select Top 5 isnull(platform, 'Total') as Platform,sum(global_sales) Total_Sales,
count(*) as #video_games
from v_video_games v
group by (platform)
order by Total_Sales desc </CommandText>
      </Query>
      <Fields>
        <Field Name="Platform">
          <DataField>Platform</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Sales">
          <DataField>Total_Sales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="ID_video_games">
          <DataField>#video_games</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
