  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>video_games1</DataSourceName>
        <CommandText>select Decade , v.sales_cases ,
sum(global_sales) Total_Sales
from v_video_games v
group by Decade,v.sales_cases  
order by   Total_Sales desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Decade">
          <DataField>Decade</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sales_cases">
          <DataField>sales_cases</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Sales">
          <DataField>Total_Sales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
